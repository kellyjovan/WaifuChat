    <div class="profile-wrap col-sm-10">
      <ul class="list-group">
        <li class="list-group-item"><span class="label label-info "><%= @characters.length %> Characters</span><a href="/new_character"><button class="btn btn-primary" type="button" style="position: absolute; right: 10px; top: 3px"> Add Character </button></a></li>
      </ul>
      <% @characters.each do |c| %>
      <% @c = c %>
      <% if c.image == nil then c.image = "http://cumbrianrun.co.uk/wp-content/uploads/2014/02/default-placeholder.png" end %>
        <div class="col-xs-6 col-md-3">
          <a href=<%= '/character/' + c.id.to_s %> class="thumbnail">
            <img src="<%= c.image %>" alt="<%= c.name %>">
          </a>
          <center><a href=<%= '/character/' + c.id.to_s %>><h3><%= c.name %></h3></a></center>
        </div>
      <% end %>
    </div>
    <div class="modal"><!-- Place at bottom of page --></div>